{
  "version": 2,
  "functions": {
    "server/api/index.ts": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/api"
    },
    {
      "source": "/health",
      "destination": "/server/api"
    }
  ],
  "buildCommand": "cd server && npm install && npm run build && cd ..",
  "outputDirectory": ".",
  "installCommand": "cd server && npm install && cd .."
}
